{
    "manifest_version": 3,
    "name": "AutoUJI by Teo2Peer",
    "description": "Inicio de sesión automático en el Aula Virtual de la UJI.",
    "version": "1.0",
    "permissions": [
        "cookies",
        "storage"
    ],
    "host_permissions": [
        "https://aulavirtual.uji.es/",
        "https://www.uji.es/"
    ],
    "optional_host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "sw.js"
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://aulavirtual.uji.es/*"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}