{
    "key": "",
    "manifest_version": 3,
    "name": "AutoLogin by Teo2Peer",
    "description": "Inicio de sesión automático en el Aula Virtual.",
    "version": "1.0",
    "permissions": [
        "tabs",
        "cookies",
        "contextMenus",
        "unlimitedStorage",
        "storage",
        "activeTab"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';",
        "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
    },
    "host_permissions": [
        "*://*.uji.es/*",
        "https://teodin.dev/*"
    ],
    "optional_host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "/js/sw.js"
        
    },
    "content_scripts": [
        {
            "js": [
                "/js/content.js",
                "/js/jquery.js"
            ],
            "css": [
                "/css/content.css"
            ],
            "all_frames": true,
            "matches": [
                "https://aulavirtual.uji.es/*",
                "https://xmlrpc.uji.es/*"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}